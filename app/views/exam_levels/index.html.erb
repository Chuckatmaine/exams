<h1>Listing exam_levels</h1>

<table>
  <tr>
    <th>Exam</th>
    <th>Level</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @exam_levels.each do |exam_level| %>
  <tr>
    <td><%= exam_level.exam_id %></td>
    <td><%= exam_level.level_id %></td>
    <td><%= link_to 'Show', exam_level %></td>
    <td><%= link_to 'Edit', edit_exam_level_path(exam_level) %></td>
    <td><%= link_to 'Destroy', exam_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

