<h1>Learning levels</h1>

<table>
  <tr>
    <th>Learning level</th>
    <th>Department</th>
    <th>Creator</th>
    <th>Locked</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @levels.each do |level| %>
  <tr>
    <td><%= level.learning_level %></td>
    <td><%= level.department_id %></td>
    <td><%= level.creator_id %></td>
    <td><%= level.locked %></td>
    <td><%= link_to 'Show', level %></td>
    <td><%= link_to 'Edit', edit_level_path(level) %></td>
    <td><%= link_to 'Destroy', level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Learning Level', new_level_path %>
