<h1>Listing exam_courses</h1>

<table>
  <tr>
    <th>Exam</th>
    <th>Course</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @exam_courses.each do |exam_course| %>
  <tr>
    <td><%= exam_course.exam_id %></td>
    <td><%= exam_course.course_id %></td>
    <td><%= link_to 'Show', exam_course %></td>
    <td><%= link_to 'Edit', edit_exam_course_path(exam_course) %></td>
    <td><%= link_to 'Destroy', exam_course, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

