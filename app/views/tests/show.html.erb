<p id="notice"><%= notice %></p>

<p>
  <b>Title:</b>
  <%= @test.title %>
</p>

<p>
  <b>Course:</b>
  <%= @test.course.name %>
</p>
<p>
<b>Level:</b>
<%= @test.level %>
</p>

<p>
  <b>Question Count:</b>
  <%= @test.question_count %>
</p>

  <ul>
    Content areas selected for this question:
  <% @test.content_areas.each do |ca| %>
      <li> <%= check_box_tag "test[content_area_ids][]", ca.id, @test.content_areas.include?(ca) %> <%=ca.name%> </li>
  <% end %>
  </ul>
 
<%= link_to 'Edit', edit_test_path(@test) %> |
<%= link_to 'Back', tests_path %>
