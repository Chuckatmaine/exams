<h1>Listing test_courses</h1>

<table>
  <tr>
    <th>Test</th>
    <th>Course</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @test_courses.each do |test_course| %>
  <tr>
    <td><%= test_course.test_id %></td>
    <td><%= test_course.course_id %></td>
    <td><%= link_to 'Show', test_course %></td>
    <td><%= link_to 'Edit', edit_test_course_path(test_course) %></td>
    <td><%= link_to 'Destroy', test_course, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

