<h1>Listing question_courses</h1>

<table>
  <tr>
    <th>Question</th>
    <th>Course</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @question_courses.each do |question_course| %>
  <tr>
    <td><%= question_course.question_id %></td>
    <td><%= question_course.course_id %></td>
    <td><%= link_to 'Show', question_course %></td>
