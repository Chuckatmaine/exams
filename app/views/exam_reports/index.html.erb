<h1>Listing exam_reports</h1>

<table>
  <tr>
    <th>Exam</th>
    <th>Report</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @exam_reports.each do |exam_report| %>
  <tr>
    <td><%= exam_report.exam %></td>
    <td><%= exam_report.report %></td>
    <td><%= link_to 'Show', exam_report %></td>
    <td><%= link_to 'Edit', edit_exam_report_path(exam_report) %></td>
    <td><%= link_to 'Destroy', exam_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

